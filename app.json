{
  "expo": {
    "name": "shadow-reader",
    "slug": "shadow-reader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/playstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["assets/**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "supportsTablet": true,
      "package": "com.github.btwnbrackets.shadowreader",
      "backgroundColor": "#5E81AC"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/playstore.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5E81AC"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
      }
    }
  }
}
